#include "main.h"
/**
 * main - _putchar - writes the character c to stdout
 * 
 * Return: On success 1
 * @int: putchar
 */
int _putchar(int);
int main(void)
{
_putchar('_');
_putchar('p');
_putchar('u');
_putchar('t');
_putchar('c');
_putchar('h');
_putchar('a');
_putchar('r');
_putchar('\n');

return (0);
}
